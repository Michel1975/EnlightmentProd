<h1>Besked Status</h1>
<div class="span11">
	<table id="sample-table-1" class="table table-striped table-bordered table-hover">
		<thead>
			<tr>
				<th>Besked id</th>
				<th>Modtager</th>
				<th>Statuskode</th>			
			</tr>
		</thead>
		<tbody>
			<% @message_notifications.each do |notification| %>
			<tr>
				<td> <%= notification.messageid %></td>
				<td> <%= notification.recipient %></td>	
				<td> <%= notification.status_code.name %></td>				
			</tr>
			<% end %>
		</tbody>
	</table>
</div>